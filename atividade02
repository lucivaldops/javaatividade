import java.util.Scanner;

public class Problem {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int A, B, soma;

        A = sc.nextInt();
        B = sc.nextInt();

        // Calculate the sum of A and B
        soma = A + B;

        // Print the result
        System.out.println("SOMA = " + soma);
    }
}
